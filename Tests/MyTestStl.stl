solid MyTestStl

// Bottom
facet normal 0.0 0.0 -1.0
    outer loop
        vertex -10.0 -10.0 0.0
        vertex 10.0 10.0 0.0
        vertex 10.0 -10.0 0.0
        vertex -10.0 -10.0 0.0
        vertex -10.0 10.0 0.0
        vertex 10.0 10.0 0.0
    endloop
endfacet

// Top
facet normal 0.0 0.0 1.0
    outer loop
        vertex -10.0 -10.0 5.0
        vertex 10.0 -10.0 5.0
        vertex 10.0 10.0 5.0
        vertex -10.0 -10.0 5.0
        vertex 10.0 10.0 5.0
        vertex -10.0 10.0 5.0
    endloop
endfacet

// -X Plane
facet normal -1.0 0.0 0.0
    outer loop
        vertex -10.0 10.0 5.0
        vertex -10.0 10.0 0.0
        vertex -10.0 -10.0 0.0
        vertex -10.0 10.0 5.0
        vertex -10.0 -10.0 0.0
        vertex -10.0 -10.0 5.0
    endloop
endfacet

// +X Plane
facet normal 1.0 0.0 0.0
    outer loop
        vertex 10.0 -10.0 5.0
        vertex 10.0 -10.0 0.0
        vertex 10.0 10.0 0.0
        vertex 10.0 -10.0 5.0
        vertex 10.0 10.0 0.0
        vertex 10.0 10.0 5.0
    endloop
endfacet

// -Y Plane
facet normal 0.0 -1.0 0.0
    outer loop
        vertex -10.0 -10.0 5.0
        vertex -10.0 -10.0 0.0
        vertex 10.0 -10.0 0.0
        vertex -10.0 -10.0 5.0
        vertex 10.0 -10.0 0.0
        vertex 10.0 -10.0 5.0
    endloop
endfacet

// +Y Plane
facet normal 0.0 -1.0 0.0
    outer loop
        vertex 10.0 10.0 5.0
        vertex 10.0 10.0 0.0
        vertex -10.0 10.0 0.0
        vertex 10.0 10.0 5.0
        vertex -10.0 10.0 0.0
        vertex -10.0 10.0 5.0
    endloop
endfacet


endsolid MyTestStl